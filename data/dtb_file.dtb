/dts-v1/;

/ {
    model = "Example Device";
    compatible = "generic,example";

    cpus {
        #address-cells = <1>;
        #size-cells = <0>;

        cpu@0 {
            device_type = "cpu";
            compatible = "generic,example-cpu";
            reg = <0>;
        };
    };

    memory {
        device_type = "memory";
        reg = <0x80000000 0x40000000>; /* 1 GB at address 0x80000000 */
    };
};
